train_net: "models/NewGeneralCLS/train.prototxt"
base_lr: 0.01
lr_policy: "step"
gamma: 0.8
stepsize: 100000
display: 100
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "New_GCLS"
